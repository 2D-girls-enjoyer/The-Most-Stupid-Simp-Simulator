{
  "name": "the-most-stupid-simp-simulator",
  "productName": "TheMostStupidSimpSimulator",
  "version": "0.0.1",
  "description": "Headpat and treat who you simp for",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package32": "electron-forge package --arch=ia32",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "win-make-squirrel": "cross-env BUILD_TARGET=squirrel electron-forge make",
    "win-make32-squirrel": "cross-env BUILD_TARGET=squirrel electron-forge make --arch=ia32",
    "linux-make-deb": "cross-env BUILD_TARGET=deb electron-forge make",
    "linux-make32-deb": "cross-env BUILD_TARGET=deb electron-forge make --arch=ia32",
    "linux-make-appimage": "cross-env BUILD_TARGET=appimage electron-forge make",
    "linux-make32-appimage": "cross-env BUILD_TARGET=appimage electron-forge make --arch=ia32",
    "linux-make-flatpak": "cross-env BUILD_TARGET=flatpak electron-forge make",
    "linux-make32-flatpak": "cross-env BUILD_TARGET=flatpak electron-forge make --arch=ia32",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-flatpak": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
